{
    "version": 2,
    "name": "awirutshop",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "header": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}